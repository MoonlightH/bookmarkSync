{
  "name": "bookmarks online sync",
  "version": "1.0",
  "manifest_version": 2,
  "description": "chrome bookmarks online sync relay on github",
  "browser_action": {
    "default_icon": {
      "19": "images/bookmark_19.png",
      "38": "images/bookmark_38.png"
    },
    "default_title": "bookmarks online sync",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "background": {
    "scripts": ["js/background.js"]
  }
}